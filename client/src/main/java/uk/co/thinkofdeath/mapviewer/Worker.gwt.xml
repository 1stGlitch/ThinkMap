<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="mapviewerworker">

    <inherits name="elemental.Elemental" />
    <inherits name="com.google.gwt.core.Core" />
    <inherits name="com.google.gwt.logging.Logging"/>

    <source path="worker"/>
    <source path="shared"/>

    <entry-point class='uk.co.thinkofdeath.mapviewer.worker.Worker'/>

    <define-linker class="uk.co.thinkofdeath.mapviewer.linker.WorkerLinker" name="webworker"/>
    <add-linker name="webworker"/>

    <set-property name="user.agent" value="gecko1_8"/>
    <set-property name="compiler.useSourceMaps" value="true"/>

    <set-property name='gwt.logging.consoleHandler' value='ENABLED'/>
    <set-property name='gwt.logging.popupHandler' value='DISABLED'/>

</module>